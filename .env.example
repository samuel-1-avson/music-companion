# ===================================
# Music Companion - Environment Config
# ===================================

# Application
NODE_ENV=production
PORT=3001

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# Supabase (PostgreSQL)
SUPABASE_URL=https://przxvdzsatzaafdvcti.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase (Frontend - Vite)
VITE_SUPABASE_URL=https://przxvdzsatzaafdvcti.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Gemini AI
GEMINI_API_KEY=your_gemini_api_key_here

# Spotify OAuth (configure in Supabase Dashboard)
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Discord OAuth (configure in Supabase Dashboard)
DISCORD_CLIENT_ID=your_discord_client_id_here
DISCORD_CLIENT_SECRET=your_discord_client_secret_here

# Telegram Bot (for notifications)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Twitch OAuth (optional)
TWITCH_CLIENT_ID=
TWITCH_CLIENT_SECRET=

# Last.fm (optional)
LASTFM_API_KEY=
LASTFM_API_SECRET=

# YouTube (optional - uses yt-dlp for downloads)
YOUTUBE_API_KEY=
